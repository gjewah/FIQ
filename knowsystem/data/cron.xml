<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="cron_auto_archive_knowsystem" model="ir.cron">
            <field name="name">[KnowSystem] Automatically archive articles</field>
            <field name="model_id" ref="model_knowsystem_article"/>
            <field name="state">code</field>
            <field name="code">model.action_auto_archive_articles()</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
        </record>

    </data>
</odoo>
